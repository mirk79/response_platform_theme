options {
  # Show page uids in page tree
  pageTree.showPageIdWithTitle = 1
  # Adds a clear-cache button for the system-cache
  clearCache.system = 1
  # Allow the editor to clear the page cache
  clearCache.pages = 1
  # Allow the editor to clear all caches
  clearCache.all = 1
}


admPanel {
  # Caching der externen TS-Files deaktivieren
  override.tsdebug.forceTemplateParsing = 1
}
